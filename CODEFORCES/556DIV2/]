#include <bits/stdc++.h>
#define endl '\n'
#define IOS ios_base::sync_with_stdio(false); cin.tie(NULL);

using namespace std;

int bool e_composto[400000];
int vet[400000];
void crivo(unsigned long long n){
	e_composto[1] = 1; 
	int k = 0;
	for(unsigned long long i = 2; i <= n; ++i){
		if(!e_composto[i]){
			vet[k++] = i;
			for(unsigned long long j = 2; j*i <= n; ++j){
				e_composto[i*j] = 1;
			}
		}
	}
	return;
}


int main(){
	IOS;
	int n,v[200000];
	int soma = 0;
	cin >> n;
	for(int i = 0 ; i < n ; ++i){
		 cin >> v[i];
		 soma += v[i];
	}
	
	crivo(soma);
	for(int i = 0 ; i < n ; ++i)
	return 0;
}

